{
  "name": "Agent Config v1.1",
  "model": "TheBloke/TinyLlama-1.1B-Chat-v1.0-GGUF",
  "system_prompt": "You are an advanced coding assistant capable of generating and explaining code using ChatML format.",
  "parameters": {
    "temperature": 0.7,
    "max_new_tokens": 4096,
    "top_p": 0.9,
    "presence_penalty": 0.0,
    "frequency_penalty": 0.0
  },
  "user_prompt": "",
  "response_format": "text",
  "metadata": {
    "prompt_format": "ChatML (for Qwen)",
    "structured_output": {
      "enabled": true,
      "schema_file": "./schemas/code_output.json"
    },
    "agent_settings": {
      "enable_code_execution": true,
      "safe_mode": true,
      "stream": true,
      "show_token_count": true,
      "show_thinking": true
    },
    "project_workspace": {
      "projects_dir": "./projects",
      "data_dir": "./data",
      "output_dir": "./output",
      "logs_dir": "./logs"
    }
  }
}